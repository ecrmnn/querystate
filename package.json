    "url": "https://github.com/ecrmnn/querystate/issues"
  },
  "homepage": "https://github.com/ecrmnn/querystate",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.0.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.5.0",
    "mocha": "^3.4.2"
